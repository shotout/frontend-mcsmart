{
  "name": "mcsmart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-prod": "react-native run-android --variant=release",
    "ios": "npx react-native run-ios --simulator \"iPhone 14\"",
    "ipad": "npx react-native run-ios --device \"iPad Gian\"",
    "link-font": "npx react-native-asset",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "clean": "cd android && ./gradlew clean",
    "pod": "rm -rf ios/Pods ios/Podfile.lock && cd ios && pod install",
    "releaseApk": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease && open app/build/outputs/bundle/release"
  },
